{
  "name": "@alalay/joybox-ui",
  "version": "0.1.7",
  "private": false,
  "repository": "https://github.com/alalay/joybox-ui",
  "dependencies": {
    "@types/prismjs": "^1.16.1",
    "polished": "^3.6.5",
    "prismjs": "^1.21.0",
    "styled-components": "^5.1.1"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "tsc -p tsconfig.build.json",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook --no-dll --quiet",
    "coverage": "react-scripts test --coverage --watchAll=false",
    "coverall": "npm run coverage  && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "np": {
    "yarn": false,
    "non-interactive": false,
    "cleanup": false
  },
  "jest": {
    "collectCoverageFrom": [
      "!src/**/*.stories.tsx",
      "src/**/*.tsx",
      "!src/**/*.snap"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@storybook/addon-a11y": "^6.0.7",
    "@storybook/addon-actions": "^6.0.7",
    "@storybook/addon-docs": "^6.0.7",
    "@storybook/addon-essentials": "^6.0.7",
    "@storybook/addon-knobs": "^6.0.7",
    "@storybook/addon-links": "^6.0.7",
    "@storybook/addon-storysource": "^6.0.7",
    "@storybook/addon-viewport": "^6.0.7",
    "@storybook/node-logger": "^6.0.7",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^6.0.7",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^10.4.9",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/styled-components": "^5.1.2",
    "babel-loader": "^8.1.0",
    "coveralls": "^3.1.0",
    "jest-styled-components": "^7.0.3",
    "np": "^6.4.0",
    "plop": "^2.7.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1",
    "react-scripts": "3.4.3",
    "typescript": "~3.7.2"
  },
  "files": [
    "dist"
  ],
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@alalay"
  }
}
